{
  "name": "coog-bench",
  "description": "Coog bench web app",
  "version": "1.11.0",
  "license": "GPL-3.0+",
  "author": "Coopengo <dev@coopengo.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/coopengo/coog-bench.git"
  },
  "scripts": {
    "beautify": "js-beautify --config .jsbeautifyrc -r *.js $(find src -name '*.js'); html-beautify --config .jsbeautifyrc -r $(find src -name '*.tpl'); css-beautify --config .jsbeautifyrc -r $(find src -name '*.css')",
    "lint": "jshint -c .jshintrc *.js $(find src -name '*.js')",
    "build": "webpack",
    "start": "webpack --watch & browser-sync start -c bs-config.js"
  },
  "dependencies": {
    "backbone": "^1",
    "backbone.marionette": "^3",
    "backbone.syphon": "^0",
    "bootstrap": "^3",
    "jquery": "^3",
    "tryton-session": "~4.2",
    "underscore": "^1"
  },
  "devDependencies": {
    "browser-sync": "^2",
    "css-loader": "^0",
    "ejs-loader": "^0",
    "extract-text-webpack-plugin": "^2",
    "file-loader": "^0",
    "webpack": "^2"
  }
}
